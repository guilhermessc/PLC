package lista_6;

public class Conta {
	private double saldo;
	private double limite;
	
	public Conta () {
		this.saldo = 0;
		this.limite = -1;
	}
	
	public double getSaldo() {
		return saldo;
	}

	public void deposita(double saldo) {
		this.saldo += saldo;
	}
	
	public void saca (double saldo) {
		this.saldo -= saldo;
	}
	
}
